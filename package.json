    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "vue": "^2.5.2",
    "vue-i18n": "^7.5.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
